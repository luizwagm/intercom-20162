<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.teclick.intercom" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Intercom Nordeste</name>
    <description>
        Programação completa da Intercom Nordeste 2016
    </description>
    <author email="luiz@teclick.com.br" href="http://teclick.com.br">
        Teclick - Por uma web criativa
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <splash height="1136" src="res/Default-568h@2x.png" width="640" />
        <splash height="1334" src="res/Default-667h@2x.png" width="750" />
        <splash height="1242" src="res/Default-Landscape-736h@3x.png" width="2208" />
        <splash height="2208" src="res/Default-Portrait-736h@3x.png" width="1242" />
        <splash height="960" src="res/Default@2x.png" width="640" />
        <icon height="60" src="res/icon/ios/icon-60.png" width="60" />
        <icon height="120" src="res/icon-60@2x.png" width="120" />
        <icon height="76" src="res/icon-76.png" width="76" />
        <icon height="152" src="res/icon-76@2x.png" width="152" />
        <icon height="40" src="res/icon-40.png" width="40" />
        <icon height="80" src="res/icon-40@2x.png" width="80" />
        <icon height="57" src="res/icon.png" width="57" />
        <icon height="114" src="res/icon@2x.png" width="114" />
        <icon height="72" src="res/icon-72.png" width="72" />
        <icon height="144" src="res/icon-72@2x.png" width="144" />
        <icon height="29" src="res/icon-Small.png" width="29" />
        <icon height="58" src="res/icon-Small@2x.png" width="58" />
        <icon height="50" src="res/icon-Small-50.png" width="50" />
        <icon height="100" src="res/icon-Small-50@2x.png" width="100" />
    </platform>
    <icon src="res/icon.png" />
    <preference name="Orientation" value="portrait" />
</widget>
