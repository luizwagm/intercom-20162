<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.teclick.intercom" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Intercom Nordeste</name>
    <description>
        Programação completa da Intercom Nordeste 2016
    </description>
    <author email="luiz@teclick.com.br" href="http://teclick.com.br">
        Teclick - Por uma web criativa
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
     <icon src="res/icon.png" />
     <preference name="Orientation" value="portrait" />
</widget>
